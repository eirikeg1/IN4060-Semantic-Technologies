@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix fam: <http://www.ifi.uio.no/IN3060/family#> .
@prefix sim: <http://www.ifi.uio.no/IN3060/simpsons#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


fam:TheSimpsons a fam:Family ;
    foaf:familyName "The Simpsons" ;
    foaf:member sim:Homer,
                sim:Marge,
                sim:Bart,
                sim:Lisa,
                sim:Maggie .

sim:Homer a foaf:Person ;
    foaf:familyName "The Simpsons" ;
    foaf:name "Homer Simpson" ;
    foaf:age "36"^^xsd:int ;
    fam:hasChild sim:Bart ;
    fam:hasChild sim:Lisa ;
    fam:hasChild sim:Maggie ;
    fam:hasSpouse sim:Marge .


sim:Marge a foaf:Person ;
    foaf:name "Marge Simpson" ;
    foaf:age "34"^^xsd:int ;
    fam:hasChild sim:Bart ;
    fam:hasChild sim:Lisa ;
    fam:hasChild sim:Maggie ;
    fam:hasSpouse sim:Homer .

sim:Bart a foaf:Person ;
    foaf:name "Bart Simpson" ;
    foaf:age "10"^^xsd:int ;
    fam:hasSister sim:Lisa ;
    fam:hasSister sim:Maggie ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge .

sim:Lisa a foaf:Person ;
    foaf:name "Lisa Simpson" ;
    foaf:age "8"^^xsd:int ;
    fam:hasBrother sim:Bart ;
    fam:hasSister sim:Maggie ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge .

sim:Maggie a foaf:Person ;
    foaf:name "Maggie Simpson" ;
    foaf:age "1"^^xsd:int ;
    fam:hasBrother sim:Bart ;
    fam:hasSister sim:Lisa ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge .


# Exercise 4
sim:Maggie fam:hasParent [
        fam:hasFather sim:Abraham
    ],
    [
        fam:hasMother sim:Mona
    ] .

# Excercise 5
sim:Lisa fam:hasParent [
    sim:hasSister sim:Patty,
                  sim:Selma
] .
